<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.attendancesystem">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ClassAttendance">
        <activity android:name="com.example.attendancesystem.LoginActivity"></activity>
        <activity android:name="com.example.attendancesystem.AttendanceActivity" />
        <activity android:name="com.example.attendancesystem.SheetActivity" />
        <activity android:name="com.example.attendancesystem.SheetListActivity" />
        <activity android:name="com.example.attendancesystem.StudentActivity" />
        <activity android:name="com.example.attendancesystem.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>